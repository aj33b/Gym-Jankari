<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="BorderPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gymjankari_v1.HomePage.MainViewController">
   <top>
      <JFXToolbar style="-fx-background-color: #003366;" BorderPane.alignment="CENTER">
         <center>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@../images/main_logo.png" />
               </image>
            </ImageView>
         </center>
         <left>
            <JFXButton fx:id="backButton" contentDisplay="GRAPHIC_ONLY" onAction="#backButtonClicked" text="Back" BorderPane.alignment="CENTER">
               <graphic>
                  <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/back_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
         </left>
      </JFXToolbar>
   </top>
   <left>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <center>
            <VBox style="-fx-background-color: #097054;" BorderPane.alignment="CENTER">
               <children>
                  <JFXButton fx:id="viewmemberButton" alignment="TOP_LEFT" layoutX="10.0" layoutY="10.0" onAction="#viewmemberButtonClicked" prefHeight="50.0" prefWidth="205.0" text="View Member" textFill="WHITE">
                     <font>
                        <Font name="CapsuulaRegular" size="25.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/search_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </JFXButton>
                  <JFXButton fx:id="addmemberButton" alignment="TOP_LEFT" onAction="#addmemberButtonClicked" prefHeight="50.0" prefWidth="205.0" text="Add Member" textFill="WHITE">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/add_user_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <font>
                        <Font name="CapsuulaRegular" size="25.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="expirydetailsButton" alignment="TOP_LEFT" layoutX="10.0" layoutY="160.0" onAction="#expirydetailsButtonClicked" prefHeight="50.0" prefWidth="205.0" text="Expiry Details" textFill="WHITE">
                     <font>
                        <Font name="CapsuulaRegular" size="25.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/expiry_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </JFXButton>
               </children>
            </VBox>
         </center>
         <bottom>
            <VBox alignment="BOTTOM_LEFT" prefHeight="387.0" prefWidth="205.0" style="-fx-background-color: #097054;" BorderPane.alignment="CENTER">
               <children>
                  <JFXButton fx:id="aboutdevButton" alignment="BOTTOM_LEFT" onAction="#aboutdevButtonClicked" prefHeight="50.0" prefWidth="205.0" text="About Developers" textFill="WHITE">
                     <font>
                        <Font name="CapsuulaRegular" size="22.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/user_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </JFXButton>
               </children>
            </VBox>
         </bottom>
      </BorderPane>
   </left>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</BorderPane>
