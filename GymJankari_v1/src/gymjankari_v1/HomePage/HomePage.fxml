<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="BorderPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gymjankari_v1.HomePage.HomePageController">
   <center>
      <GridPane style="-fx-background-color: #ffffff;" layoutX="25.0" layoutY="14.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <JFXButton fx:id="addmemberButton" contentDisplay="TOP" onAction="#addmemberclicked" text="Add Member" GridPane.halignment="CENTER">
               <graphic>
                  <ImageView fx:id="addmemberImageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/add_user_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="CapsuulaRegular" size="25.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="editmemberButton" contentDisplay="TOP" text="Edit Member" GridPane.columnIndex="1" GridPane.halignment="CENTER">
               <graphic>
                  <ImageView fx:id="editmemberImageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/edit_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="CapsuulaRegular" size="25.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="searchmemberButton" contentDisplay="TOP" text="Search Member" GridPane.columnIndex="2" GridPane.halignment="CENTER">
               <graphic>
                  <ImageView fx:id="searchmemberImageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/search_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="CapsuulaRegular" size="25.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="paymentdetailsButton" contentDisplay="TOP" text="Payment Details" GridPane.halignment="CENTER" GridPane.rowIndex="1">
               <graphic>
                  <ImageView fx:id="paymentdetailsImageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/payment_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="CapsuulaRegular" size="25.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="expirydetailsButton" contentDisplay="TOP" text="Expiry Details" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
               <graphic>
                  <ImageView fx:id="expirydetailsImageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/expiry_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="CapsuulaRegular" size="25.0" />
               </font>
            </JFXButton>
         </children>
      </GridPane>
   </center>
</BorderPane>
